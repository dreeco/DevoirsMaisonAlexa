{
  "metadata": [
    {
      "namespaceLayout": "nested",
      "src": [
        {
          "src": "../",
          "files": [
            "**/*.csproj",
            "**/bin/Release/DevoirsAlexa.*.dll"
          ],
          "exclude": [
            "**/*.Tests.csproj",
            "**/bin/Release/DevoirsAlexa.Tests.dll"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "resources/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Devoirs Maison",
      "_appTitle": "Devoirs Maison",
      "_enableSearch": true,
      "pdf": false
    }
  }
}